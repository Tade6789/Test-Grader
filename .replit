modules = ["web", "python-3.11", "postgresql-16"]
[agent]
expertMode = true
integrations = ["stripe:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["cairo", "ffmpeg-full", "freetype", "ghostscript", "glibcLocales", "gobject-introspection", "gtk3", "openssl", "pkg-config", "postgresql", "qhull", "tcl", "tk"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "webserver"

[[workflows.workflow]]
name = "webserver"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python wed_view.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 33165
externalPort = 3000

[[ports]]
localPort = 33639
externalPort = 4200

[[ports]]
localPort = 35291
externalPort = 3001

[[ports]]
localPort = 35649
externalPort = 3003

[[ports]]
localPort = 41593
externalPort = 3002

[[ports]]
localPort = 46127
externalPort = 5000

[deployment]
deploymentTarget = "autoscale"
run = ["python", "wed_view.py"]
